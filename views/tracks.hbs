<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Listen</th>
        </tr>
    </thead>
    <tbody>
        {{#each tracks}}
        <tr>
            <td>{{this.name}}</td>
            <td> <audio src="{{this.preview_url}}"></audio> </td>
        </tr>
        {{/each}} 
    </tbody>
</table>